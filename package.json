{
  "name": "utm-dot-codes",
  "version": "1.7.0",
  "description": "A plugin that makes building analytics friendly links quick and easy.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "delete-maps": "find ./ -name '*.map' -delete",
    "dev": "npm run development",
    "dev-dashboard": "npm run development-dashboard",
    "development": "NODE_ENV=development webpack --config=webpack.config.js --info-verbosity verbose --progress --watch",
